    <Color x:Key="ComboBox.Border" R="152" G="152" B="152" A="255"/>
    <Color x:Key="ComboBox.Glyph" R="152" G="152" B="152" A="255"/>
    <Color x:Key="ComboBox.Border.MouseOver" R="76" G="76" B="76" A="255"/>
    <Color x:Key="ComboBox.Glyph.MouseOver" R="76" G="76" B="76" A="255"/>
    <Color x:Key="ComboBox.Border.Pressed" R="0" G="0" B="0" A="255"/>
    <Color x:Key="ComboBox.Glyph.Pressed" R="0" G="0" B="0" A="255"/>
    <Color x:Key="ComboBox.Background.Disabled" R="206" G="206" B="206" A="255"/>
