<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="Window.Background" R="255" G="255" B="255" A="255"/>
    <Color x:Key="Window.Title.Background" R="188" G="188" B="188" A="255"/>
    <Color x:Key="Window.Title.Foreground" R="255" G="255" B="255" A="255"/>

    <Color x:Key="Button.Background.MouseOver" R="168" G="168" B="168" A="255"/>
    <Color x:Key="Button.Background.Pressed" R="148" G="148" B="148" A="255"/>

    <SolidColorBrush x:Key="Window.Background.Static" Color="{StaticResource Window.Background}"/>
    <SolidColorBrush x:Key="Window.Title.Background.Static" Color="{StaticResource Window.Title.Background}"/>
    <SolidColorBrush x:Key="Window.Title.Foreground.Static" Color="{StaticResource Window.Title.Foreground}"/>

    <SolidColorBrush x:Key="Button.MouseOver.Background" Color="{StaticResource Button.Background.MouseOver}"/>
    <SolidColorBrush x:Key="Button.Pressed.Background" Color="{StaticResource Button.Background.Pressed}"/>
</ResourceDictionary>