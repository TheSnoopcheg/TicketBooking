<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="Window.Background" R="255" G="255" B="255" A="255"/>
    <Color x:Key="Window.Title.Background" R="148" G="148" B="148" A="255"/>
    <Color x:Key="Window.Title.Foreground" R="255" G="255" B="255" A="255"/>

    <Color x:Key="Button.Background.MouseOver" R="128" G="128" B="128" A="255"/>
    <Color x:Key="Button.Background.Pressed" R="108" G="108" B="108" A="255"/>
    <Color x:Key="Button1.Background" R="52" G="97" B="235" A="255"/>
    <Color x:Key="Button1.Background.MouseOver" R="52" G="97" B="207" A="255"/>
    <Color x:Key="Button1.Background.Pressed" R="52" G="97" B="187" A="255"/>
    <Color x:Key="Button1.Background.Disabled" R="97" G="97" B="97" A="255"/>
