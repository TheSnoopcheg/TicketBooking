    <SolidColorBrush x:Key="ComboBox.Static.Background" Color="{StaticResource ComboBox.Background}"/>
    <SolidColorBrush x:Key="ComboBox.Static.Border" Color="{StaticResource ComboBox.Border}"/>
    <SolidColorBrush x:Key="ComboBox.Static.Glyph" Color="{StaticResource ComboBox.Glyph}"/>
    <SolidColorBrush x:Key="ComboBox.MouseOver.Border" Color="{StaticResource ComboBox.Border.MouseOver}"/>
    <SolidColorBrush x:Key="ComboBox.MouseOver.Glyph" Color="{StaticResource ComboBox.Glyph.MouseOver}"/>
    <SolidColorBrush x:Key="ComboBox.Pressed.Border" Color="{StaticResource ComboBox.Border.Pressed}"/>
    <SolidColorBrush x:Key="ComboBox.Pressed.Glyph" Color="{StaticResource ComboBox.Glyph.Pressed}"/>
    <SolidColorBrush x:Key="ComboBox.Disabled.Background" Color="{StaticResource ComboBox.Background.Disabled}"/>
    <SolidColorBrush x:Key="ComboBox.Disabled.Border" Color="{StaticResource ComboBox.Border.Disabled}"/>
    <SolidColorBrush x:Key="ComboBox.Disabled.Glyph" Color="{StaticResource ComboBox.Glyph.Disabled}"/>
    <SolidColorBrush x:Key="ComboBox.Item.Static.Background" Color="{StaticResource ComboBox.Item.Background}"/>
    <SolidColorBrush x:Key="ComboBox.Item.NouseOver.Background" Color="{StaticResource ComboBox.Item.Background.MouseOver}"/>
